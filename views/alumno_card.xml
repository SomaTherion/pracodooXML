<?xml version="1.0" encoding="utf-8" ?>
<odoo>
  <template id="report_alumno">
    <t t-call="web.html_container">
      <t-foreach="docs" t-as="alu">
        <t t-call="web.external_layout">
          <h2>Informe de Alumno</h2>
          <t t-esc="alu.dni" />
          <t t-esc="alu.nombre" />
          <t t-esc="alu.apellidos" />
          <t t-esc="alu.provincia" />
        </t>  
      </t>
    </t>
  </template>  
</odoo>

